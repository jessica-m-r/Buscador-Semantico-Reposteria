<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/reposteria#"
     xml:base="http://example.org/reposteria"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:reposteria="http://example.org/reposteria#">

  <owl:Ontology rdf:about="http://example.org/reposteria"/>

  <!-- Clases -->
  <owl:Class rdf:about="http://example.org/reposteria#Postre"/>
  <owl:Class rdf:about="http://example.org/reposteria#Ingrediente"/>
  <owl:Class rdf:about="http://example.org/reposteria#Tecnica"/>
  <owl:Class rdf:about="http://example.org/reposteria#Utensilio"/>
  <owl:Class rdf:about="http://example.org/reposteria#Pais"/>
  <owl:Class rdf:about="http://example.org/reposteria#CategoriaIngrediente"/>

  <!-- Propiedades de objeto -->
  <owl:ObjectProperty rdf:about="http://example.org/reposteria#requiereIngrediente">
    <rdfs:domain rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:range rdf:resource="http://example.org/reposteria#Ingrediente"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/reposteria#usaTecnica">
    <rdfs:domain rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:range rdf:resource="http://example.org/reposteria#Tecnica"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/reposteria#requiereUtensilio">
    <rdfs:domain rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:range rdf:resource="http://example.org/reposteria#Utensilio"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/reposteria#esTipicoDe">
    <rdfs:domain rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:range rdf:resource="http://example.org/reposteria#Pais"/>
  </owl:ObjectProperty>

  <!-- Propiedades de datos -->
  <owl:DatatypeProperty rdf:about="http://example.org/reposteria#tieneCalorias">
    <rdfs:domain rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/reposteria#tieneTiempoPreparacion">
    <rdfs:domain rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Ejemplo de instancias -->
  <owl:NamedIndividual rdf:about="http://example.org/reposteria#Ejemplo_Torta">
    <rdf:type rdf:resource="http://example.org/reposteria#Postre"/>
    <rdfs:label xml:lang="es">Torta de Ejemplo</rdfs:label>
    <rdfs:label xml:lang="en">Example Cake</rdfs:label>
    <reposteria:tieneTiempoPreparacion rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PT01H30M</reposteria:tieneTiempoPreparacion>
  </owl:NamedIndividual>

</rdf:RDF>
